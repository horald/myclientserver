<Connected>:
    BoxLayout:
        orientation: 'vertical'

        Label:
            text: "Die Anmeldung war erfolgreich."
            font_size: 32

        Label:
            id: anzuser
            text: "(Anz User)"
            font_size: 12

        Button:
			size_hint: 0.30, 1
            pos_hint:{"center_x": 0.5, "center_y":0.5}
			center: self.parent.center
			background_color: (0.5, 0, 0, 1.0)
            font_size: 29
            text: "Abmelden"
            on_press: root.logOff()